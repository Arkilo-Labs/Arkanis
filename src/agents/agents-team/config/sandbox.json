{
    "version": 1,
    "provider_id": "oci_local",
    "engine": "auto",
    "runtime": "auto",
    "mode": "sandboxed",
    "image": "node:20-bookworm-slim",
    "workspace_access": "none",
    "network_policy": "off",
    "mounts": [],
    "resources": {
        "max_wall_clock_ms": 60000,
        "max_stdout_bytes": 4194304,
        "max_stderr_bytes": 1048576,
        "max_memory_mb": 512,
        "max_cpu": 1,
        "max_processes": 512
    }
}
