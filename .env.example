// VLM Trade JS 环境变量配置示例
// 复制此文件为 .env 并填入实际值

// PostgreSQL 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=skeet
DB_DATABASE=market_signals
DB_POOL_MIN=2
DB_POOL_MAX=10

// VLM 配置
// Prompt 模板名称（位于 prompts 目录）
PROMPT_NAME=default

// 图表配置
CHART_WIDTH=1280
CHART_HEIGHT=720
CHART_VOLUME_PANE_HEIGHT=0.2
CHART_MACD_PANE_HEIGHT=0.15
CHART_TREND_STRENGTH_PANE_HEIGHT=0.15

// 日志配置
// 可选值: debug, info, warn, error, silent
LOG_LEVEL=info

// 默认参数
DEFAULT_SYMBOL=BTCUSDT
DEFAULT_TIMEFRAME=5m
DEFAULT_BARS=200
